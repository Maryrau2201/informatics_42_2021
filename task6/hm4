#include <iostream>
#include <cmath>

#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int x, y;
    cin >> x >> y;
    int z = x;
    int i = y;
    if (y > x)
    {
        y = y % x; 
    }
    else 
    {
        x = x % y;
    }

    while ((x != 0) && (y != 0));
    cout << z / (x + y) * i;
}
